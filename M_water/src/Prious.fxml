<AnchorPane prefHeight="489.0" prefWidth="799.0" stylesheets="@application/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SubController">
   <children>
      <TableView fx:id="waterTable" layoutX="20.0" layoutY="150.0" prefHeight="304.0" prefWidth="759.0">
