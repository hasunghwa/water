      <ComboBox fx:id="nameBOX" layoutX="20.0" layoutY="77.0" prefHeight="37.0" prefWidth="190.0" />
      <Button fx:id="btn" layoutX="706.0" layoutY="14.0" mnemonicParsing="false" onAction="#movePage" prefHeight="43.0" prefWidth="73.0" style="-fx-background-color: #3fc5f0;" styleClass="button3" text="안녕">
